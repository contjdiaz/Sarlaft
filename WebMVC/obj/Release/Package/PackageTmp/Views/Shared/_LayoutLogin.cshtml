<!DOCTYPE html>
<html>
<head>
    <title>SARLAFT</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
	<link rel="icon" href="~/img/favicon.png" type="image/x-icon" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="page-container">
        <div class="page-header">
            <!-- page header holder -->
            <!-- page logo -->

            <div class="row fondo_icetex">
                <div class="col-xs-5 logo_icetex">
                    <img src="~/img/logos/logo_icetex.png" title="Icetex" alt="Icetex" class="img-responsive" />
                </div>
                <div class="col-xs-7">
                    <div class="row">
                        <div class="col-sm-4 col-sm-push-8 col-xs-6 col-xs-push-7 nuevopais">

                        </div>
                        <div class="col-sm-8 col-sm-pull-6 col-xs-12 col-xs-pull-5  titulo_icetex">
                            FORMULARIO SARLAFT
                        </div>
                    </div>
                </div>
            </div>

        </div>
     
            <div class="page-content">
                @RenderBody()
            </div>
                <hr />
                <footer>
                    @*<p>&copy; @DateTime.Now.Year - SARLAFT</p>*@
                </footer>
            </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
</body>
</html>
